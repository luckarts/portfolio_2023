{
  "name": "my-react-demo-project",
  "private": true,
  "version": "0.0.0",
  "proxy": "http://localhost:5000/",
  "scripts": {
    "build-scss": "node scripts/getCssFile.js",
    "build:style": "postcss ./src/assets/style/main.scss -o ./src/assets/style/tailwind.scss --watch",
    "importCss": "nodemon --watch app/**/**/*.scss --ext scss --exec \"npm run build-scss\"",
    "watch:css": "nodemon --watch ./style/main.scss -e css --exec \"npm run build:style\"",
    "dev": "vite",
    "build": "tsc && vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview"
  },
  "devDependencies": {
    "@fullhuman/postcss-purgecss": "^5.0.0",
    "@types/react": "^18.2.15",
    "@types/react-dom": "^18.2.7",
    "@types/react-helmet": "^6.1.6",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "@vitejs/plugin-react-swc": "^3.3.2",
    "autoprefixer": "^10.4.15",
    "dotenv": "^16.3.1",
    "eslint": "^8.48.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-config-react-app": "^7.0.1",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.3",
    "eslint-plugin-testing-library": "^6.0.1",
    "node-sass": "^9.0.0",
    "postcss-import": "^15.1.0",
    "postcss-nesting": "^12.0.1",
    "prettier": "^3.0.3",
    "prop-type": "^0.0.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-helmet": "^6.1.0",
    "react-hook-form": "^7.45.4",
    "react-i18next": "^13.2.2",
    "react-query": "^3.39.3",
    "react-swipeable": "^7.0.1",
    "sass": "^1.66.1",
    "tailwindcss-animation-delay": "^1.0.7",
    "tailwindcss-animations": "^2.0.0",
    "tailwindcss-gradients": "^3.0.0",
    "typescript": "^5.0.2",
    "typescript-eslint": "^0.0.1-alpha.0",
    "vite": "^4.4.5",
    "vite-plugin-checker": "^0.6.2"
  },
  "dependencies": {
    "@headlessui/react": "^1.7.17",
    "@rollup/plugin-alias": "^5.0.0",
    "axios": "^1.5.0",
    "classnames": "^2.3.2",
    "html-react-parser": "^4.2.2",
    "i18next-browser-languagedetector": "^7.1.0",
    "i18next-http-backend": "^2.2.2",
    "path": "^0.12.7",
    "postcss-cli": "^10.1.0",
    "react-icons": "^4.11.0",
    "react-router-dom": "^6.15.0",
    "ts-node": "^10.9.1"
  }
}
